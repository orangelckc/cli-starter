    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-jwt": "^8.4.1",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "mysql2": "^3.6.5",
    "node-schedule": "^2.1.1",
    "redis": "^4.6.11",
    "sequelize": "^6.35.2",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "winston-mysql": "^1.1.1"
