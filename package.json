    "lint-staged": "^15.2.0",
    "multer": "1.4.5-lts.1",
    "nodemon": "^3.0.2",
    "sequelize-automate": "^1.2.2",
    "simple-git-hooks": "^2.9.0",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
